  "files": [
    "dist/",
    "i18n.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradyholt/cronstrue.git"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "chai": "^3.5.0",
    "expose-loader": "^0.7.1",
    "handlebars": "^4.0.6",
    "marked": "^0.3.6",
    "mocha": "^2.5.3",
    "npm-github-release": "^0.7.3",
    "octonode": "^0.7.6",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "ts-loader": "^2.0.3"
  },
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
